/*** Products ***/
.product-list.loading {
    position: relative;
    min-height: 100px;
}
.product-list.loading:before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
    background: url('../images/img-loading.gif') no-repeat center center;
    background-size: 150px auto;
    background-color: rgba(255, 255, 255, 0.5);
}

.list-product,
.list-recruitment {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    gap: 21px;
}

.item-product,
.item-recruitment {
    /*-ms-flex: 0 0 calc(20% - 21px);
    flex: 0 0 calc(20% - 21px);
    max-width: calc(20% - 21px);*/
    border: 1px solid var(--gray-dot);
    border-radius: 8px;
    height: 100%;
    display: flex;
}
.item-product>a{
    display: flex;
    flex-direction: column;
    flex: 1;
    width: 100%;
}

.bottom-wrapper {
    border-left: 3px solid transparent;
}
.item-product .bottom-wrapper {
    flex: 1;
}

.item-product a:hover .bottom-wrapper,
.item-services a:hover .bottom-wrapper,
.item-recruitment a:hover .bottom-wrapper {
    border-left-color: var(--primary);
    border-radius: 0px 0px 0px 8px;
    background: linear-gradient(90deg, #FFF8E0 0%, rgba(255, 248, 224, 0.00) 100%);
}

.thumb-product,
.thumb-recruitment {
    position: relative;
    overflow: hidden;
    border-top-right-radius: 8px;
    border-top-left-radius: 8px;
}
.thumb-product:before{
    content: "";
    padding-top: 70%;
    display: block;
}
.thumb-product picture {

}
.thumb-product img {
    height: 100%;
    -o-object-fit: cover;
    object-fit: cover;
    position: absolute;
/*    width: 100%;*/
    top: 0;
    left: 50%;
    transform: translateX(-50%);
}

.thumb-product img,
.thumb-recruitment img {
/*    width: 100%;*/
}

.thumb-product .badge,
.thumb-recruitment .badge {
    position: absolute;
    top: 10%;
    left: -35px;
    display: inline-block;
    font-size: 12px;
    font-weight: 600;
    line-height: 14px;
    width: 110px;
    text-align: center;
    padding: 4px 0;
