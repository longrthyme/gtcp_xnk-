    font-weight: normal;
}

.price-location {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    max-width: 203px;
}

.item-product.vieclam-item > a {
    flex-direction: row;
}
.item-product.vieclam-item > a:hover{
    background: linear-gradient(90deg, #FFF8E0 0%, rgba(255, 248, 224, 0.00) 100%);
}
.item-product.vieclam-item .thumb-product {
    width: 100px;
    border-radius: 0;
