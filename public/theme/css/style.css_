/********** Template CSS **********/
:root {
    --primary: #004E99;
    --orange: #FF6B00;
    --red: #FF0000;
    --light: #F8F8F8;
    --black: #000000;
    --dark: #1C1C1C;
    --green: #A1CC2C;
    --white: #ffffff;
    --warning: #664C00;
    --success: #104E00;
    --gray: #191F33;
    --gray-scale: #464D61;
    --gray-dot: #d9d9d9;
    --gray-des: #636A80;
    --bg-primary: #E8F7FF;
    --bg-gray: #F6F6F6;
    --bg-success: #E4F9E0;
    --bg-warning: #FFF2CC;
    --bg-danger: #FFCCCC;
}

body{
    font-family: 'Nunito', sans-serif;
}

h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
    font-family: 'Inter', sans-serif;
}

h1,
h2,
.h1,
.h2,
.fw-bold {
    font-weight: 600;
}

h3,
h4,
.h3,
.h4,
.fw-medium {
    font-weight: 500;
}

h5,
h6,
.h5,
.h6,
.fw-normal {
    font-weight: 400;
}

p {
    margin-bottom: 0;
}

a {
    text-decoration: none;
}

img {
    max-width: 100%;
    height: auto;
}

ul {
    list-style: none;
    padding: 0;
    margin: 0;
}

input+.error{
  font-size: 13px;
  color: #f00;
  margin-top: 5px;
}

button:focus,
.btn-close:focus {
    box-shadow: unset;
    outline: none;
}

.form-select:focus ,
.form-control:focus {
    box-shadow: unset;
    outline: none;
}

.back-to-top {
    position: fixed;
    display: none;
    right: 30px;
    bottom: 30px;
    z-index: 99;
    background: var(--primary);
    color: #ffffff;
}

.back-to-top:focus  {
    box-shadow: none;
}

.back-to-top:hover {
    color: #ffffff;
    background: var(--orange);
}

.bg-brown {
    background: var(--primary);
}

.fs-sm {
    font-size: 14px;
}

.container .container {
    padding-left: 0;
    padding-right: 0;
}

.input-group div.error {
    position: absolute;
    bottom: -20px;
    
}


@media(min-width: 1200px)
{
    .col-20{
        flex: 0 0 20%;
        max-width: 20%;
    }
}


/*** Button ***/
.btn {
    font-weight: 500;
    transition: .5s;
}


.btn-custom,
.dangtin-process{
    font-size: 1rem;
    line-height: 1.25;
    padding: 9px 19px;
    border-radius: 0;
    min-width: 250px;
    color: #fff;
    text-transform: uppercase;
    background: var(--orange);
}
.btn-custom:hover,
.dangtin-process:hover{
    opacity: 0.8;
    color: #fff;
    background: var(--orange);
}
.btn-custom.btn-custom-secondary {
    background: #979797;
}
.btn-custom.btn-custom-secondary:hover {
    background: #6c757d;
}

.btn-group .btn-custom{
    flex: 1;
    min-width: unset;
}


.btn.btn-primary,
.btn.btn-secondary {
    color: #FFFFFF;
}

.btn-square {
    width: 38px;
    height: 38px;
}

.btn-sm-square {
    width: 32px;
    height: 32px;
}

.btn-lg-square {
    width: 48px;
    height: 48px;
}

.btn-square,
.btn-sm-square,
.btn-lg-square {
    padding: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: normal;
}

.btn-outline-body {
    color: var(--primary);
    border-color: #777777;
}

.btn-outline-body:hover {
    color: #FFFFFF;
    background: var(--primary);
    border-color: var(--primary);
}

.btn-product {
    background-color: #A1CC2C;
    color: #ffffff;
    font-weight: 500;
    font-size: 16px;
    line-height: 20px;
    padding: 10px 24px;
    transition: all 0.3s ease-in-out;
    overflow: hidden;
    position: relative;
}

.btn-product::before, .list-category a::before, .blog-content a.view::before, .add-cart a::before,  .cart-block a::before, .checkout-btn button::before {
  background: #fff;
  content: "";
  height: 155px;
  opacity: 0;
  position: absolute;
  top: -50px;
  transform: rotate(35deg);
  width: 50px;
  transition: all 3000ms cubic-bezier(0.19, 1, 0.22, 1);
  left: -50%;
}

.btn-product::after, .list-category a::after, .blog-content a.view::after, .add-cart a::after, .cart-block a::after, .checkout-btn button::after {
  background: #fff;
  content: "";
  height: 20rem;
  opacity: 0;
  position: absolute;
  top: -50px;
  transform: rotate(35deg);
  transition: all 3000ms cubic-bezier(0.19, 1, 0.22, 1);
  width: 8rem;
  left: -100%;
}

.btn-product:hover, .btn-product:active, .list-category a:hover, .blog-content a.view:hover, .add-cart a:hover, .cart-block a:hover, .checkout-btn button:hover  {
    color: #ffffff;
    transform: translateY(-3px);
    box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.3);
}

.btn-product:hover::after, .list-category a:hover::after, .blog-content a.view:hover::after, .add-cart a:hover::after, .cart-block a:hover::after, .checkout-btn button:hover::after {
    left: 200%;
    opacity: 0.6;
}

.topbar ul {
    display: flex;
    align-items: center;
    gap: 15px;
}

.topbar a {
    color: var(--white);
    font-size: 14px;
    font-weight: 400;
    line-height: 20px;
}

.topbar a:hover {
    color: var(--orange);
}

.topbar a.topbar-link {
    line-height: 40px;
}

.top-auth-dropdown .dropdown-menu {
    min-width: 190px;
}

.login-user {
    align-items: center;
    padding: 0 16px;
    background: var(--orange);
    border-radius: 0;
}
.btn.login-user:hover {
    background: var(--orange);
    opacity: 0.9;
}
.login-user a {
    font-size: 16px;
    font-weight: 500;
    line-height: 40px;
    color: var(--white);
    height: 40px;
}
.login-user a:hover {
    color: var(--white);
}


.navbar-nav .nav-item {
    position: relative;
}

.navbar-nav .nav-item .nav-link {
    text-transform: uppercase;
    color: var(--dark);
    font-family: 'Inter', sans-serif;
    line-height: 24px;
}

.navbar-nav .nav-item .nav-link.active,
.navbar-nav .nav-item .nav-link:hover {
    color: var(--primary);
}

.navbar-brand img{
    max-height: 60px;
}

.shop-currency a{
    display: block;
    padding: 0 10px;
}

.header-left,
.header-right {
    display: flex;
    align-items: center;
}

.header-right {
    gap: 28px;
}


.dropdown .dropdown-menu {
    display: none;
}
.dropdown .dropdown-menu .dropdown-menu {
    left: 100%;
    top: 0;
}
.dropdown:hover>.dropdown-menu {
    display: block;
    -webkit-animation: slide-up .25s ease-in-out;
    animation: slide-up .25s ease-in-out;
}

.navbar .nav-item .dropdown-menu li {
    cursor: pointer;
}

.navbar .nav-item .dropdown-menu li:hover {
    color: var(--primary);
}

.currency {
    padding: 9px 11px;
    border: 1px solid var(--primary);
}

.menu-bg {
    background: white;
}
.menu-bg .navbar-nav .nav-item .nav-link {
    font-weight:bold;
    color: white;
}
.menu-bg .currency{
    color: white;
    border-color: #fff;
}

@media(min-width: 991px)
{
    .header-nav .dropdown-toggle::after{
        transform: rotate(-90deg);
        /*position: absolute;
        right: 6px;
        top: 43%;*/
    }
}

@media(max-width: 1366px)
{
    .navbar-nav .nav-item .nav-link{
        font-size: 13px;
    }
}
@media(max-width: 1221px)
{
    .navbar-brand img {
        max-height: 60px;
    }
}
@media(min-width: 900px) and (max-width: 1220px)
{
    .navbar-nav .nav-item .nav-link{
        text-transform: capitalize;
    }
    .header-right {
        gap: 5px;
    }
    .currency {
        padding: 5px;
    }
}
@media(max-width: 767px)
{
    .navbar-brand img {
        max-height: 40px;
    }
    .menu-bg .navbar-nav .nav-item .nav-link {
    	color: white;
        font-weight:bold;
    }
}


@-webkit-keyframes fade-in {
    from {
        opacity: 0
    }

    to {
        opacity: 1
    }
}

@keyframes fade-in {
    from {
        opacity: 0
    }

    to {
        opacity: 1
    }
}

@-webkit-keyframes slide-up {
    from {
        transform: translateY(0.5rem);
        opacity: 0
    }

    to {
        transform: translateY(0);
        opacity: 1
    }
}

@keyframes slide-up {
    from {
        transform: translateY(0.5rem);
        opacity: 0
    }

    to {
        transform: translateY(0);
        opacity: 1
    }
}

/*.offcanvas*/
.offcanvas-header,
.offcanvas-body {
    width: 100%;
}
.offcanvas-body {
    padding: 10px 0;
}
.offcanvas .navbar-nav .nav-link{
    padding-left: 10px;
    padding-right: 10px;

}

.m-menu-item {
    background: #fff;
    width: 100%;
}

.nav-toggle {
    position: absolute;
    top: 0;
    right: 0;
    height: 32px;
    width: 30px;
    display: none;
    align-items: center;
    justify-content: center;
}
.nav-toggle .nav-toggle {
    height: 30px;
}
.header-nav .dropdown-toggle.nav-toggle::after {
    position: relative;
    top: unset;
    right: unset;
}

.dropdown-menu .nav-toggle {
    display: flex;
}

@media(max-width: 990px)
{
    .nav-toggle {
        height: 40px;
        width: 40px;
        display: flex;
    }
    .dropdown-menu .nav-toggle {
        
        height: 30px;
    }
    .navbar .dropdown .dropdown-menu {
        border: 0;
        border-radius: 0;
        background: #f1f1f1;
        padding: 5px 0 5px 15px;
    }
}
