/*password toggle*/
.password-toggle {
    position: relative
}

.password-toggle .form-control {
    padding-right: 3rem
}

.password-toggle-btn {
    position: absolute;
    top: 50%;
    right: .625rem;
    margin-bottom: 0;
    padding: .5rem;
    transform: translateY(-50%);
    font-size: 1.0625rem;
    line-height: 1;
    cursor: pointer
}

.password-toggle-btn .password-toggle-indicator {
    transition: color .2s ease-in-out;
    color: #7d879c;
    font-family: "cartzilla-icons";
    font-style: normal
}

.password-toggle-btn .password-toggle-indicator::before {
    content: "î¤·"
}

