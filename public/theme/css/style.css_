/*** Carousel ***/

.owl-carousel-inner {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    display: flex;
    align-items: center;
    width: 100%;
    height: 100%;
}

.owl-carousel-inner h2 {
    font-size: 56px;
    line-height: 67px;
    text-transform: capitalize;
    color: var(--gray);
    margin-bottom: 24px;
}

.owl-carousel-inner h2 span {
    color: var(--primary);
}

.owl-carousel-inner p {
    line-height: 24px;
    color: var(--gray-scale);
}

.header-carousel .owl-dots {
    position: relative;
    left: 50%;
    transform: translateX(-50%);
    bottom: 45px;
    display: flex;
    justify-content: center;
    margin: 0 auto;
    padding: 0 12px;
}

.header-carousel .owl-dots .owl-dot span {
    width: 12px;
    height: 12px;
    border: 1px solid var(--gray-dot);
    background: var(--gray-dot);
    display: block;
    margin: 5px;
    -webkit-backface-visibility: visible;
    transition: opacity .2s ease;
    border-radius: 30px;
}

.header-carousel .owl-dot.active span {
    background: var(--primary);
    border-color: var(--primary);
}

/*** Search Cate ***/
.block-search {
    margin-bottom: 20px;
}

.search-feature .form-search {
    position: relative;
    display: flex;
    border: 1px solid var(--gray-dot);
}

.search-feature .form-search::after {
    content: '';
    display: block;
    clear: both;
}

.input-search {
    position: relative;
    height: 52px;
    flex: 1;
    border-left: 1px solid var(--gray-dot);
    border-right: 1px solid var(--gray-dot);
    margin-left: -1px;
    margin-right: -1px;
/*    max-width: 200px;*/
}
.input-search:first-child {
    max-width: unset;
}

.input-search i {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 10px;
}

.input-search {
    max-width: 300px;
}
.input-w-auto.input-search {
    max-width: unset;
}
.input-search.input-search-small {
    max-width: 220px;
}

.search-feature .input-search input {
    height: 100%;
    font-size: 16px;
    
    padding: 6px 12px;
    border: 0;
    border-radius: 0;
}
.search-feature .input-search.icon input{
    padding: 6px 12px 6px 30px;
}
.search-feature .input-search:first-child {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

input[type=search]:focus {
    box-shadow: none;
}

.search-feature .select2-container {
    max-width: 100%;
    width: 100% !important;
}

