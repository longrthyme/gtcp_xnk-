/*invite-box*/
.invite-box {
    margin: auto;
    width: 510px;
    max-width: 100%;
    padding: 30px;
    border-radius: 10px;
    background: rgb(11, 14, 17);
    font-size: 14px;
}
.invite-box div{
    background: #5e6673;
    padding: 7px 20px;
    border-radius: 20px;
    display: flex;
    justify-content: space-between;
    margin-bottom: 10px;
    color: rgb(234, 236, 239);
}
.invite-box > a {
    width: 100%;
    text-align: center;
    display: block;
    padding: 10px 20px;
    background: rgb(252, 213, 53);
    color: rgb(24, 26, 32);
    border-radius: 20px;
    font-size: 16px;
}
.invite-box div>span {
    position: relative;
    padding-right: 20px;
}

.invite-box .copy {
    position: absolute;
    top: 2px;
    right: 0;
    background: url(../images/icon-copy.svg) no-repeat center center;
    display: inline-block;
    width: 12px;
    height: 14px;
    cursor: pointer;
}
.invite-box .copy.active {
    background: url(../images/icon-copy-success.svg) no-repeat center center;
}
.invite-box span i {
    color: #fcd535;
    cursor: pointer;
}
@media(max-width: 767px)
{
    .invite-box div>span span {
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
        max-width: 100px;
        display: block;
    }
}
